{
  "name": "msdbox",
  "version": "0.1.8",
  "description": "A tiny UMD execution utility with a pure-JS interpreter (for environments that disallow eval/new Function).",
  "keywords": [
    "interpreter",
    "javascript",
    "sandbox",
    "umd",
    "eval",
    "execution",
    "safe",
    "secure",
    "runtime",
    "vm",
    "isolated",
    "csp",
    "content-security-policy",
    "mini-program",
    "wechat",
    "commonjs",
    "module"
  ],
  "license": "MIT",
  "type": "commonjs",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "build": "tsup",
    "prune:dist": "node scripts/prune-dist.cjs",
    "prepack": "npm run clean && npm run build && npm run prune:dist",
    "publish:npm": "node scripts/publish-npm.cjs",
    "publish:github": "node scripts/publish-github-packages.cjs"
  },
  "dependencies": {
    "acorn": "^8.14.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/estree": "^1.0.6",
    "tsup": "^8.3.6",
    "typescript": "^5.7.2"
  }
}

