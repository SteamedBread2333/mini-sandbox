{
  "name": "mini-sandbox",
  "version": "0.1.0",
  "description": "A tiny UMD execution utility with a pure-JS interpreter (for environments that disallow eval/new Function).",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./umd": {
      "types": "./dist/umd/index.d.ts",
      "import": "./dist/umd/index.js",
      "require": "./dist/umd/index.cjs"
    }
  },
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "build": "tsup",
    "prepack": "npm run clean && npm run build"
  },
  "dependencies": {
    "acorn": "^8.14.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/estree": "^1.0.6",
    "tsup": "^8.3.6",
    "typescript": "^5.7.2"
  }
}

